{{#each this}}
    <div class="page-content">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet light ">
                    <ul class="page-breadcrumb breadcrumb">
                        <li>
                            <a href="index.html"><font key="i18n.site"></font></a>
                            <i class="fa fa-circle"></i>
                        </li>
                        <li>
                            <span class="active"><font key="i18n.groupManagement"></font></span>
                        </li>
                    </ul>

                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font key="i18n.groupManagement"></font> </span>
                            </div>
                            <!--按钮-->
                            <div class="actions">
                                <div class="btn-group btn-group-devided " data-toggle="buttons">
                                    <button class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25" data-toggle="modal" data-target="#myModal">
                                        <input type="radio" name="options" class="toggle"  > <font key="i18n.createGroup"></font></button>
                                    <label class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="deleteGroup">
                                        <input type="radio" name="options" class="toggle" ><font key="i18n.deleteGroup"></font></label>
                                    <label class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="addMemberToGroup">
                                        <input type="radio" name="options" class="toggle" id="option3"><font key="i18n.addGroup"></font></label>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 ">
                                <form class="form-inline actions" role="form">
                                    <div class="form-group col-sm-4 col-md-4  ">
                                        <input type="text" class="form-control" size="30" placeholder="按ID搜索" id="groupId">
                                    </div>
                                    <div class="form-group col-sm-4 col-md-4 ">
                                        <input type="text" class="form-control "  size="30" placeholder="按名称搜索" id="groupName">
                                    </div>
                                    <!--<span class="input-group-btn "><button class="btn " type="button">搜索</button></span>-->
                                    <div class="form-group col-sm-4 col-md-2 ">
                                        <button type="button" class="btn green col-md-8 col-ms-8 col-xs-4 col-xs-offset-8" id="check_group"><font key="i18n.search"></font></button>
                                    </div>

                                </form>
                            </div>
                            <!--按钮效果-->
                            <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">×</span>
                                            </button>
                                            <!--<h4 class="modal-title" id="myModalLabel">组</h4>-->
                                            <div class="caption font-green bordered modal-title">
                                                <i class="icon-settings font-green"></i>
                                                <span class="caption-subject bold uppercase"> <font key="i18n.group"></font></span>
                                            </div>
                                        </div>
                                        <div class="modal-body">
                                            <div class="nav nav-tabs">
                                                <div class="mt-element-step" >
                                                    <div class="row step-line ">
                                                        <div class="col-md-3 mt-step-col first active" id="nav_tab1" style="padding: 0 !important;">
                                                            <div class="mt-step-number bg-white">1</div>
                                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.group"></font></div>
                                                        </div>
                                                        <div class="col-md-3 mt-step-col last" id="nav_tab2" style="padding: 0 !important;">
                                                            <div class="mt-step-number bg-white" style="color:#e5e5e5;" >2</div>
                                                            <div class="mt-step-title uppercase font-grey-cascade" ><font key="i18n.member"></font></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="tab-content">

                                                <div class="tab-pane active" id="tab1">
                                                    <form class="form-horizontal" role="form" id="myModalGroupForm">
                                                        <div class="form-group">
                                                            <label for="memberId" class="col-md-3 control-label" >ID:<span class="required">*</span></label>

                                                            <div class="col-md-6">
                                                                <input type="text" class="form-control" id="memberId" placeholder="请输入ID">
                                                            </div>
                                                        </div>
                                                        <div class="form-group">

                                                            <label for="name" class="col-md-3 control-label" ><font key="i18n.DocName"></font>:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <input type="text" class="form-control" id="name" placeholder="请输入名称">
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="desc" class="col-md-3 control-label" ><font key="i18n.description"></font>:</label>
                                                            <div class="col-md-6">
                                                                <textarea class="form-control" rows="3" id="desc" placeholder="请输入说明"></textarea>
                                                            </div>
                                                        </div>
                                                    </form>
                                                    <div class="modal-footer">
                                                        <button type="button" class="btn btn-success next-btn" id="next-btn"><font key="i18n.nextStep"></font></button>
                                                    </div>
                                                </div>

                                                <div class="tab-pane" id="tab2" >
                                                    <div class="row">
                                                        <div class="col-lg-12 col-xs-12 col-sm-12">
                                                            <div class="portlet light bordered">
                                                                <div class="portlet-title">

                                                                    <div class="actions">
                                                                        <a href="#" class="btn btn-circle green btn-outline btn-sm" id="addMem">
                                                                            <i class="fa fa-user-plus"></i> <font key="i18n.add"></font></a>
                                                                        <a href="#" class="btn btn-circle green btn-outline btn-sm" id="deleteMem">
                                                                            <i class="fa fa-user-times"></i> <font key="i18n.delete"></font> </a>
                                                                    </div>
                                                                </div>
                                                                <div class="portlet-body">
                                                                    <!--<div class="huai">2</div>-->
                                                                    <!--<input type="text" value="mashr" id="ccc">-->
                                                                    <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="Memberlist">
                                                                        <thead>
                                                                        <tr class="bg-green text-white" >
                                                                            <th class="col-md-1 col-sm-1 col-xs-1">
                                                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                                                                                    <input type="checkbox" class="group-checkable item_check_one"  data-set="#example .checkboxes" />
                                                                                    <span></span>
                                                                                </label>
                                                                            </th>

                                                                            <th>ID</th>
                                                                            <th> <font key="i18n.DocName"></font> </th>
                                                                            <th> <font key="i18n.description"></font></th>

                                                                        </tr>
                                                                        </thead>
                                                                        <tbody id="tbMain">

                                                                        </tbody>

                                                                    </table>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="modal-footer">
                                                        <button type="button" class="btn btn-success up-btn"><font key="i18n.preStep"></font></button>
                                                        <button type="button" class="btn btn-primary  saveGroupMem"><font key="i18n.SAVE"></font></button>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>

                        </div>


                        <!--表单组体-->
                        <div class="portlet-body">

                            <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="group_one">
                                <thead>
                                <tr class="bg-green text-white" >
                                    <th class="col-md-1 col-sm-1 col-xs-1">
                                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                                            <input type="checkbox" class="group-checkable item_check"   />
                                            <span></span>
                                        </label>
                                    </th>
                                    <th>ID</th>
                                    <th> <font key="i18n.DocName"></font> </th>
                                    <th> <font key="i18n.description"></font> </th>

                                </tr>
                                </thead>

                            </table>
                        </div>
                    </div>
                    <!-- END EXAMPLE TABLE PORTLET-->
                </div>
            </div>
        </div>
    </div>

{{/each}}