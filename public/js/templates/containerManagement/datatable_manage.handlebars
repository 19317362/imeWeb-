{{!-- datatable --}}
{{#each this}}
<div class="col-md-12">
    <div class="portlet light bordered">
        <div class="portlet-title">
            <div class="caption font-green bordered">
                <i class="icon-settings font-green"></i>
                <span class="caption-subject bold uppercase"> <font key="i18n.containerManagement"></font> </span>
            </div>
            <div class="actions">
                <div class="btn-group btn-group-devided" data-toggle="buttons">
                    <button class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="create_containerBtn">
                    <input type="radio" name="options" class="toggle" >新建存储库
                  </button>
                   <button class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="edit_containerBtn">
                    <input type="radio" name="options" class="toggle" >编辑存储库
                  </button>
                  <button class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="delete_containerBtn">
                    <input type="radio" name="options" class="toggle" >删除存储库
                  </button>
                  <button class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="copy_containerBtn">
                    <input type="radio" name="options" class="toggle" >复制存储库结构
                  </button>
             </div>
         </div>
        </div>
        <div class="portlet-body" style="min-height: 500px;">
            <div class="container_listAlert">

            </div>
            <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="containerManagement_table">
                <thead>
                <tr class="bg-green text-white" >
                    <th class="col-md-1 col-sm-1 col-xs-1">
                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                            <input type="checkbox" class="group-checkable item_check" id="item_check" data-set="#example .checkboxes" />
                            <span></span>
                        </label>
                    </th>
                    <th>  <font key="i18n.name"></font>  </th>
                    <th> <font key="i18n.descriptioin"></font> </th>
                    <th><font key="i18n.status"></font></th>
                </tr>
                </thead>
                <tbody>

                </tbody>
            </table>
        </div>
    </div>
</div>
{{/each}}