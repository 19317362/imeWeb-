{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}}">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> 复制存储库</span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                        <div class="portlet-body form">
                            <div class="container_alert"></div>
                            <form class="form-horizontal" role="form">
                                <div class="form-body myform-body">
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">目标存储库</label>
                                        <div class="col-md-5">
                                            <input type="text" class="form-control" id="targetContainer">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-md-3 control-label">选择模板存储库</label>
                                        <div class="col-md-5">
                                            <div class="input-group">
                                                <input type="text" class="form-control" id="modelContainer" >
                                                <div class="input-group-btn">
                                                    <button type="button" class="btn btn-success dropdown-toggle search_container" data-toggle="dropdown">
                                                        <i class="fa fa-angle-down"></i>
                                                    </button>
                                                    <ul class="dropdown-menu pull-right " id="container_copyData">
                                                       <!-- 插入模板-->
                                                        <!--<li>-->
                                                            <!--<a href="javascript:;"> <font key="i18n.group">组</font> </a>-->
                                                        <!--</li>-->
                                                        <!--<li>-->
                                                            <!--<a href="javascript:;"> <font key="i18n.user">用户</font> </a>-->
                                                        <!--</li>-->
                                                    </ul>

                                                </div>
                                                <!-- /btn-group -->
                                            </div>
                                        </div>
                                    </div>


                                </div>
                                <div class="modal-footer mymodal-footer">
                                    <button type="button"  class="btn default btn-outline" onclick="imeWeb.deleteChildPage();">取消</button>
                                    <button type="button" class="btn green" id="{{id}}">确定</button>
                                </div>
                            </form>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}

