{{#each this}}
<div class="page-content parent-{{parentName}} {{pageName}}">
    <div class='row'>
        <!--表格插入区域-->
        <div class="col-md-12">
            <div class="portlet light bordered">
               <div class="portlet-title tabbable-line">
                <div class="caption">
                   <button type="button" class="btn  mt-ladda-btn ladda-button myblockbutton  blockui_TaskInit" data-style="contract" data-spinner-color="#697882">
                    <span class="ladda-label"> <i class="icon-refresh"></i></span>
                    <span class="ladda-spinner"></span>
                    <span class="ladda-spinner"></span>
                    <span class="ladda-spinner"></span>
                </button>
                <span class="caption-subject font-dark bold uppercase">对象查询</span>
            </div>

            <div class="actions">
              {{> common/protletTitleTools protletTitleTools}}

          </div>

      </div>
      <div class="portlet-body" style="min-height:300px">
        <form class="form-horizontal" role="form">
            <div class="form-body myform-body">
                <div class='row'>

                    <div class="form-group col-md-4">
                        <label class="col-md-3 control-label">关键字</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id='search_object_title'>
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label class="col-md-3 control-label">内容</label>
                        <div class="col-md-9">
                            <input type="text" class="form-control" id='search_object_content'>
                        </div>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="control-label col-md-3">创建时间起止</label>
                        <div class="col-md-9">
                            <div class="input-group  date-picker input-daterange" data-orientation="right" data-date="10/11/2012" >
                                <input type="text" class="form-control" name="from" id='search_object_begincreated'>
                                <span class="input-group-addon"> 至</span>
                                <input type="text" class="form-control" name="to" id='search_object_endcreated'>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-md-4">
                        <label class="col-md-3 control-label">创建人</label>
                        <div class="col-md-9">
                            <div class="input-group">
                                <input  class="form-control" type="text" name="password"   id='search_object_Principal'>
                                <span class="input-group-btn">
                                    <button id="search_object_searchPrincipal" class="btn btn-success" type="button"> 选择
                                    </button>
                                </span>
                            </div>
                        </div>
                    </div>

                    <div class="form-group col-md-4">
                        <label class="col-md-3 control-label">存储库</label>
                        <div class="col-md-9">
                          <div class="input-group">
                            <input type="text" class="form-control" id="search_object_container">
                            <div class="input-group-btn">
                              <button type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-angle-down"></i>
                            </button>
                            <ul class="dropdown-menu pull-right  my_select"  id="search_object_containerlist">

                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="form-group col-md-4">
                <label class="col-md-3 control-label">类型</label>
                <div class="col-md-9">
                  <div class="input-group">
                    <input type="text" class="form-control" id="search_object_sort">
                    <div class="input-group-btn">
                      <button type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown">
                        <i class="fa fa-angle-down"></i>
                    </button>
                    <ul class="dropdown-menu pull-right  my_select"  id="search_object_sortlist">

                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="form-group col-md-4">
        <label class="col-md-3 control-label">检出人</label>
        <div class="col-md-9">
            <div class="input-group">
                <input id="search_object_checkoutUser" class="form-control" type="text" name="password" >
                <span class="input-group-btn">
                    <button id="search_object_searchcheckoutUser" class="btn btn-success" type="button"> 选择
                    </button>
                </span>
            </div>
        </div>
    </div>
    <div class="form-group col-md-4">
        <label class="control-label col-md-3"></label>
        <div class="col-md-3">

        </div>
        <div class="col-md-9">
           <button type="submit" class="btn btn-success mt-ladda-btn ladda-button search_object " id="search_object_search" data-style="zoom-in">
            <span class="ladda-label">
                <i class="fa fa-search"></i>  搜索
            </span>
            <span class="ladda-spinner">
            </span>
            <div class="ladda-progress" style="width: 0px;">
            </div>
        </button>

    </div>
</div>

</div>
</div>
</form>
<div id="search_object_refresharea">
    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="search_object">
        <thead>

        </thead>
        <tbody>

        </tbody>
    </table>
</div>
</div>
</div>
</div>
</div>
</div>
{{/each}}

