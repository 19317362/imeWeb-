<!--流程实例 -->
{{#each this}}
<div class="portlet-body" id='table_search_show'>
    <form class="form-horizontal" role="form" id="tab_instance_form">
        <div class="form-body myform-body">
            <div class='row'>
                <div class="form-group col-md-4">
                    <label class="col-md-3 control-label"></label>
                    <div class="col-md-9">
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="col-md-3 control-label"></label>
                    <div class="col-md-9">

                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="col-md-3 control-label"></label>
                    <div class="col-md-9">
                        <div class="btn-group btn-group-devided  pull-right" data-toggle="buttons">
                            <button class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 " id="tab_instance_startprocess">
                                <input type="radio" name="options" class="toggle" >启动流程
                            </button>
                            <button class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 ">
                                <input type="radio" name="options" class="toggle" >终止流程
                            </button>
                        </div>
                    </div>
                </div>
            </div>
            <div class='row'>

                <div class="form-group col-md-4">
                    <label class="col-md-3 control-label">名称</label>
                    <div class="col-md-9">
                        <div class="input-group" >
                            <input type="text" class="form-control my_input" id="tab_instance_name">
                            <div class="input-group-btn" >
                                <button type="button" class="btn btn-success dropdown-toggle"  data-toggle="dropdown">
                                    <i class="fa fa-angle-down"></i>
                                </button>
                                <ul class="dropdown-menu pull-right my_select"  id="tab_instance_namelist">

                                </ul>

                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group col-md-4">
                    <label class="col-md-3 control-label">ID</label>
                    <div class="col-md-9">
                        <input type="text" class="form-control" id="tab_instance_ID">
                    </div>
                </div>

              <div class="form-group col-md-4">
                <label class="col-md-3 control-label">状态</label>
                <div class="col-md-9">
                    <div class="input-group">
                        <input type="text" class="form-control my_input" id="tab_instance_status">
                        <div class="input-group-btn">
                            <button type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-angle-down"></i>
                            </button>
                            <ul class="dropdown-menu pull-right my_select" id="tab_instance_statuslist">
                              <li><a href="javascript:;">全部</a></li>
                              <li><a href="javascript:;">任务开始</a></li>
                              <li><a href="javascript:;">任务完成</a></li>
                              <li><a href="javascript:;">取消</a></li>

                          </ul>

                      </div>
                  </div>
              </div>
          </div>

          <div class="form-group col-md-4">
            <label class="control-label col-md-3">创建时间起止</label>
            <div class="col-md-9">
                <div class="input-group  date-picker input-daterange" data-orientation="right" data-date="10/11/2012" >
                    <input type="text" class="form-control" name="from" id="tab_instance_timefrom">
                    <span class="input-group-addon"> 至</span>
                    <input type="text" class="form-control" name="to" id="tab_instance_timeto">
                </div>
            </div>
        </div>
        <div class="form-group col-md-4">
            <label class="control-label col-md-3"></label>
            <div class="col-md-3">
                <button type="submit" class="btn btn-success mt-ladda-btn ladda-button" id="tab_instance_search" data-style="zoom-in">
                    <span class="ladda-label">
                        <i class="fa fa-search"></i>  搜索
                    </span>
                    <span class="ladda-spinner">
                    </span>
                    <div class="ladda-progress" style="width: 0px;">
                    </div>
                </button>
            </div>
        </div>
    </div>
</div>
</form>
<table class="table table-striped table-bordered table-hover table-checkable order-column" id="table_search">
    <thead>

    </thead>
    <tbody>

    </tbody>
</table>
</div>

{{/each}}