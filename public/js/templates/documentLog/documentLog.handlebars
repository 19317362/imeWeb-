{{#each this}}
    <div class="page-content">
        <div class='row'>
            <div class="col-md-12">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <button type="button" class="btn  mt-ladda-btn ladda-button myblockbutton  blockui_TaskInit"
                                    data-style="contract" data-spinner-color="#697882">
                                <span class="ladda-label"> <i class="icon-refresh"></i></span>
                                <span class="ladda-spinner"></span>
                                <span class="ladda-spinner"></span>
                                <span class="ladda-spinner"></span>
                            </button>
                            <span class="caption-subject font-dark bold uppercase">文档日志</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <form class="form-horizontal" role="form">
                            <div class="form-body myform-body">
                                <div class='row'>

                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">用户ID</label>
                                        <div class="col-md-9">
                                            <input id="docLog_userIdInput" type="text" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">名称</label>
                                        <div class="col-md-9">
                                            <input id="docLog_docNameInput" type="text" class="form-control">
                                        </div>
                                    </div>

                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">编号</label>
                                        <div class="col-md-9">
                                            <input id="docLog_docNumInput" type="text" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">类型</label>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input id="docLog_softtypeInput" chosetype="" type="text" class="form-control" id="input" value="">
                                                <div class="input-group-btn">
                                                    <button id="docLog_drapDown_softtype" type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" >
                                                        <i class="fa fa-angle-down"></i>
                                                    </button>
                                                    <ul class="dropdown-menu pull-right" id="docLog_softtype_showDate"style="max-height: 200px;overflow-y: scroll;">
                                                        <!--插入-->
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">操作类型</label>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input id="docLog_actiontypeInput" chosetype="" type="text" class="form-control" id="input" value="">
                                                <div class="input-group-btn">
                                                    <button id="docLog_drapDown_actionType" type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" >
                                                        <i class="fa fa-angle-down"></i>
                                                    </button>
                                                    <ul id="docLog_actiontype_dataShow" class="dropdown-menu pull-right" style="max-height: 200px;overflow-y: scroll;">
                                                        <li c_value="CHECKOUT">
                                                            <a href="javascript:;">检出</a>
                                                        </li>
                                                        <li c_value="CHECKIN">
                                                            <a href="javascript:;">检入</a>
                                                        </li>
                                                        <li c_value="CANCELCHECKOUT">
                                                            <a href="javascript:;">取消检出</a>
                                                        </li>
                                                        <li c_value="REVISE">
                                                            <a href="javascript:;">升版</a>
                                                        </li>
                                                        <li c_value="OPEN">
                                                            <a href="javascript:;">打开</a>
                                                        </li>
                                                        <li c_value="SAVE">
                                                            <a href="javascript:;">保存</a>
                                                        </li>
                                                        <li c_value="SAVEAS">
                                                            <a href="javascript:;">另存为</a>
                                                        </li>
                                                        <li c_value="UPLOAD">
                                                            <a href="javascript:;">上传</a>
                                                        </li>
                                                        <li c_value="OPENXML">
                                                            <a href="javascript:;">XML打开</a>
                                                        </li>
                                                        <li c_value="DELETE">
                                                            <a href="javascript:;">删除</a>
                                                        </li>
                                                        <li c_value="LOGIN">
                                                            <a href="javascript:;">登录</a>
                                                        </li>
                                                        <li c_value="LOGOUT">
                                                            <a href="javascript:;">登出</a>
                                                        </li>
                                                        <li c_value="DOWNLOAD">
                                                            <a href="javascript:;">下载</a>
                                                        </li>
                                                        <li c_value="LogEXPORT">
                                                            <a href="javascript:;">系统日志导出</a>
                                                        </li>
                                                        <li c_value="UserCREATE">
                                                            <a href="javascript:;">用户创建</a>
                                                        </li>
                                                    </ul>

                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="form-group col-md-4">
                                        <label class="control-label col-md-3">创建时间起止</label>
                                        <div class="col-md-9">
                                            <div class="input-group  date-picker input-daterange"
                                                 data-orientation="right" data-date="10/11/2012">
                                                <input id="docLog_begincreated" type="text" class="form-control"
                                                       name="from">
                                                <span class="input-group-addon"> 至</span>
                                                <input id="docLog_endcreated" type="text" class="form-control"
                                                       name="to">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <form class="form-horizontal" role="form">
                            <div class="form-group col-md-4" style="height: 0">
                            </div>
                            <div class="form-group col-md-4" style="height: 0">
                            </div>
                            <div class="form-group col-md-4">
                                <button type="button" id="docLog_search"
                                        class="btn  green mt-ladda-btn ladda-button pull-right"
                                        data-style="slide-right" style="">
                                                <span class="ladda-label"> <i class="fa  fa-search"></i> <font
                                                        key="i18n.search"></font></span>
                                </button>
                            </div>
                        </form>
                        <div id="docLog_searchRes_loadding" style="min-height: 500px;">
                            <table id="docLog_dataShow"
                                   class="table table-striped table-bordered table-hover table-checkable order-column"
                                   id="search_object">
                                <thead>
                                <tr class="bg-green">
                                    <th>操作时间</th>
                                    <th>编号</th>
                                    <th>名称</th>
                                    <th>类型</th>
                                    <th>操作类型</th>
                                    <th>修改人</th>
                                    <th>站点信息</th>
                                    <th>ip</th>
                                </tr>
                                </thead>
                                <tbody id="docLog_searchRes">
                                <!--搜索结果数据-->
                                </tbody>

                            </table>

                            <div class="modal-footer" style="border-top: none;">
                                <button id="homeDoc_confChecked" type="button" class="btn green"><font
                                        key="i18n.conform"></font>
                                </button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}