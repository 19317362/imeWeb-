<!--工作流容器-->
{{#each this}}
    <div class="page-content  page-content-base" id='workflowManagement'>
        <ul class="page-breadcrumb breadcrumb">
            <li>
                <a href="index.html">首页</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span class="active"><font>翻译配置</font></span>
            </li>
        </ul>
        <div class='row'>
            <!--表格插入区域-->
            <div class="col-md-12">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <button type="button" class="btn  mt-ladda-btn ladda-button myblockbutton  blockui_TaskInit" data-style="contract" data-spinner-color="#697882">
                                <span class="ladda-label"> <i class="icon-refresh"></i></span>
                                <span class="ladda-spinner"></span>
                                <span class="ladda-spinner"></span>
                                <span class="ladda-spinner"></span>
                            </button>
                            <span class="caption-subject font-dark bold uppercase">翻译配置</span>
                        </div>


                        <ul class="nav nav-tabs">
                            <li class="active">
                                <a id="transLM_createLangList" data-toggle="tab">创作语言清单</a>
                            </li>
                            <li>
                                <a id="transLM_transLangList" data-toggle="tab">翻译语言清单</a>
                            </li>
                        </ul>

                    </div>
                    <div class="portlet-body" style="min-height:300px">
                        <div class="tab-content">
                            <div class="row">
                                <div class="col-lg-12 col-xs-12 col-sm-12">
                                    <div class="portlet light bordered">
                                        <div class="portlet-title">
                                            <!--四个按钮-->
                                            <div class="actions">
                                                <div class="btn-group btn-group-devided " data-toggle="buttons">

                                                    <button type="button" id="transLM_save"
                                                            class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25 mt-ladda-btn ladda-button"
                                                            data-style="slide-right">
                                                <span class="ladda-label"><font
                                                        key="i18n.save"></font></span>
                                                    </button>

                                                    <label id="transLM_addData"
                                                           class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25">
                                                        <input type="radio" name="options" class="toggle"> <font
                                                            key="i18n.add"></font></label>
                                                    <label id="transLM_deleteBtn"
                                                           class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 ">
                                                        <input type="radio" name="options" class="toggle" id="option3"><font
                                                            key="i18n.DeleteCreate"></font></label>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="portlet-body">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <div id="homeDoc_gAlert" class="col-md-12"></div>
                                                    <!--主表格-->
                                                    <div id="transLM_mainTable_loadding" style="min-height: 500px;">
                                                        <table class="table table-striped table-bordered table-hover table-checkable order-column"
                                                               id="transLM_mianTable">
                                                            <thead>
                                                            <tr class="bg-green">
                                                                <th>
                                                                    <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                                                        <input id="transLM_allChecked" type="checkbox"
                                                                               class="group-checkable"
                                                                               data-set="#sample_1 .checkboxes"/>
                                                                        <span></span>
                                                                    </label>
                                                                </th>
                                                                <th>语言代码</th>
                                                                <th>语言说明</th>
                                                                <th>储存文件夹</th>
                                                            </tr>
                                                            </thead>
                                                            <tbody id="homeDoc_dataBox">

                                                            </tbody>
                                                        </table>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}