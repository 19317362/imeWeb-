 <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-cursor font-dark hide"></i>
                            <span class="caption-subject font-dark bold uppercase"><font
                                    key="i18n.homeDoc"></font></span>
                        </div>
                        <!--四个按钮-->
                        <div class="actions">
                            <div class="btn-group btn-group-devided " data-toggle="buttons">

                                <button type="button" id="homeDoc_save"
                                        class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25 mt-ladda-btn ladda-button"
                                        data-style="slide-right">
                                                <span class="ladda-label"><font
                                                        key="i18n.save"></font></span>
                                </button>

                                <label id="homeDoc_addData"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25">
                                    <input type="radio" name="options" class="toggle"> <font
                                        key="i18n.add"></font></label>
                                <label id="homeDoc_resetSeq"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 ">
                                    <input type="radio" name="options" class="toggle" id="option3"><font
                                        key="i18n.resetSeq"></font></label>
                                <label id="homeDoc_deleteBtn"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 ">
                                    <input type="radio" name="options" class="toggle" id="option3"><font
                                        key="i18n.DeleteCreate"></font></label>
                            </div>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div id="homeDoc_gAlert" class="col-md-12"></div>
                                <!--主表格-->
                                <div id="homeDoc_mainTable_loadding" style="min-height: 500px;">
                                    <table class="table table-striped table-bordered table-hover table-checkable order-column"
                                           id="homeDoc">
                                        <thead>
                                        <tr class="bg-green">
                                            <th>
                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                                    <input id="homeDoc_allChecked" type="checkbox"
                                                           class="group-checkable homeDoc_checkBox"
                                                           data-set="#sample_1 .checkboxes"/>
                                                    <span></span>
                                                </label>
                                            </th>
                                            <th><font key="i18n.seq"></font></th>
                                            <th><font key="i18n.DocName"></font></th>
                                            <th><font key="i18n.number"></font></th>
                                            <th><font key="i18n.created"></font></th>
                                        </tr>
                                        </thead>
                                        <tbody id="homeDoc_dataBox">

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>