<div  id="modal_createchanges" class="modal fade" tabindex="-1" data-focus-on="input:first" data-width='800'>
    <div class="portlet light mylight">
        <div class="portlet-title">
            <div class="caption">
                <i class="icon-settings font-dark"></i>
                <span class="caption-subject font-dark sbold uppercase">新建变更</span>
            </div>
            <div class="actions">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
            </div>
        </div>
        <div class="portlet-body form">
            <form class="form-horizontal" role="form">
                <div class="form-body myform-body">
                {{> modals_static text1='存储库' p1='AAA' text2="存储路径" p2='12-document'}}
                    {{> modals_select text='类型'}}
                    {{> modals_input12 text='名称'}}
                   <!--  {{> modals_input12 text='文件夹'}} -->
                   <!--  {{> modals_inputfile text='文件'}} -->

                    {{> modals_textarea text='说明'}}
                </div>
            </form>
            {{> modals_submit text='名称'}}
        </div>

    </div>

</div>