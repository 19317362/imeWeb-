{{! home页面容器 }}
{{#each this}}
    <div class="page-content page-content-base">
        {{! 面包屑导航 }}
        <ul class="page-breadcrumb breadcrumb">
            <li>
                <a href="index.html"><font key="i18n.homePage"></font></a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span class="active"><font key="i18n.siteManagement"></font></span>
            </li>
        </ul>

        <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-settings font-green"></i>
                            <span class="caption-subject font-dark bold uppercase"><font key="i18n.siteManagement"></font></span>
                        </div>
                        <!--四个按钮-->
                        <div class="actions">
                            <div class="btn-group btn-group-devided " data-toggle="buttons">

                                <label id="siteMag_addData"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25">
                                    <input type="radio" name="options" class="toggle"> <font key="i18n.addUser"></font></label>
                                <label id="siteMag_deleteBtn"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 ">
                                    <input type="radio" name="options" class="toggle" id="option3"> <font key="i18n.removeUser"></font></label>
                            </div>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div id="siteMag_gAlert" class="col-md-12"></div>
                                <!--主表格-->
                                <div id="siteMag_mainTable_loadding" style="min-height: 350px;">
                                    <table class="table table-striped table-bordered table-hover table-checkable order-column"
                                           id="siteMag_mainTable">
                                        <thead>
                                        <tr class="bg-green">
                                            <th>
                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                                    <input id="siteMag_allChecked" type="checkbox"
                                                           class="group-checkable homeDoc_checkBox"
                                                           data-set="#sample_1 .checkboxes"/>
                                                    <span></span>
                                                </label>
                                            </th>
                                            <th><font key="i18n.id"></font></th>
                                            <th><font key="i18n.name"></font></th>
                                        </tr>
                                        </thead>
                                        <tbody id="siteMag_dataBox">

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-cursor font-dark hide"></i>
                            <span class="caption-subject font-dark bold uppercase"><font key="i18n.UserManagementRights"></font></span>
                        </div>
                    </div>
                    <div class="portlet-body" id="siteMag_powerItems" style="display: none;">
                        <div class="col-lg-12 col-xs-12 col-sm-12 mt-checkbox-inline">
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="siteInfo" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.siteInfo"></font>
                            </label>

                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="groupManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.groupManagement"></font>
                            </label>

                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="containerManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.containerManagement"></font>
                            </label>

                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="siteManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.siteManagement"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="userManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.userManagement"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="workflowManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.workflowManagement"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="dataUploadManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.dataUploadManagement"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="queueManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.queueManagement"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="homeDoc" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.homeDoc"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input  name="logRecord" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.logRecord"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="translationLangManagement" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.translationLangManagement"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="systemInfo" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                <font key="i18n.systemInfo"></font>
                            </label>
                            <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                <input name="metaData" type="checkbox"
                                       class="group-checkable jurisdiction_checkBox"
                                       data-set="#sample_1 .checkboxes"/>
                                <span></span>
                                metaData
                            </label>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-md-12  ">
                            <div class="pull-right">
                                <button type="submit" class="btn green" id="siteMag_confirmBtn"><font key="i18n.confirm"></font></button>
                            </div>
                        </div>

                    </div>
                    </div>
            </div>
        </div>

    </div>

{{/each}}