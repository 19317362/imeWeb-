{{! home页面容器 }}
{{#each this}}
    <div class="page-content page-content-base">
        {{! 面包屑导航 }}
        <ul class="page-breadcrumb breadcrumb">
            <li>
                <a href="index.html">首页</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span class="active"><font>站点管理员配置</font></span>
            </li>
        </ul>

        <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-cursor font-dark hide"></i>
                            <span class="caption-subject font-dark bold uppercase">站点管理员配置</span>
                        </div>
                        <!--四个按钮-->
                        <div class="actions">
                            <div class="btn-group btn-group-devided " data-toggle="buttons">

                                <label id="siteMag_addData"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25">
                                    <input type="radio" name="options" class="toggle"> 添加用户</label>
                                <label id="siteMag_deleteBtn"
                                       class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-25 ">
                                    <input type="radio" name="options" class="toggle" id="option3">删除用户</label>
                            </div>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div id="siteMag_gAlert" class="col-md-12"></div>
                                <!--主表格-->
                                <div id="siteMag_mainTable_loadding" style="min-height: 500px;">
                                    <table class="table table-striped table-bordered table-hover table-checkable order-column"
                                           id="siteMag_mainTable">
                                        <thead>
                                        <tr class="bg-green">
                                            <th>
                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                                    <input id="siteMag_allChecked" type="checkbox"
                                                           class="group-checkable homeDoc_checkBox"
                                                           data-set="#sample_1 .checkboxes"/>
                                                    <span></span>
                                                </label>
                                            </th>
                                            <th><font key="i18n.id"></font></th>
                                            <th><font key="i18n.name"></font></th>
                                        </tr>
                                        </thead>
                                        <tbody id="siteMag_dataBox">

                                        </tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

{{/each}}