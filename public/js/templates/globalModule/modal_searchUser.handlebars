    <div  id="stackmodel_adduser" class="modal fade" tabindex="1" data-focus-on="input:first" data-width='600'>
        <div class="portlet light mylight">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-settings font-dark"></i>
                    <span class="caption-subject font-dark sbold uppercase"><font key="i18n.searchPrincipal"></font></span>
                </div>
                <div class="actions">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                </div>
            </div>
            <div class="portlet-body form">
                <form role="form">
                    <div class="form-body myform-body">
                        <div class="form-group">
                            <label><font key="i18n.softtype"></font></label>
                            <input type="text" class="form-control" id="type" value='用户' disabled>
                        </div>
                        <div class="form-group">
                            <label>ID</label>
                            <input id="homeDoc_principalId" class="form-control spinner" type="text" placeholder="">
                        </div>
                        <div class="form-group">
                            <label><font key="i18n.name"></font></label>
                            <input id="homeDoc_searchName" class="form-control spinner" type="text" placeholder="">
                        </div>
                    </div>
                    <div class="form-group">
                        <button id="homeDoc_searchUser" type="button" class="btn green"><font key="i18n.search"></font></button>
                    </div>

                </form>

            </div>


            <div class="portlet-body">
                <table class="table table-striped table-bordered table-hover table-checkable order-column"
                       id="usersDataShow">
                    <thead>
                    <tr>
                        <th class="col-md-1 col-sm-1 col-xs-1">

                        </th>
                        <th>ID</th>
                        <th><font key="i18n.name"></font></th>
                        <th><font key="i18n.descriptioin"></font></th>
                        <th><font key="i18n.softtype"></font></th>
                    </tr>
                    </thead>
                    <tbody id="modal_searchUsersBox">
                    <!--插入用户数据-->
                    </tbody>

                </table>
            </div>

            <div class="modal-footer mymodal-footer">
                <button id="modal_searchUser_close" type="button" data-dismiss="modal" class="btn btn-outline dark"><font key="i18n.wf_cancel"></font></button>
                <button id="modal_searchUser_conform" modal_searchType="{{searchType}}" type="button" class="btn green"><font key="i18n.confirm"></font></button>
            </div>
        </div>

    </div>

