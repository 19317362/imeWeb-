{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}}">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font key="i18n.group"></font></span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                        <div class="portlet-body">
                            <div id = "user_listAlert"></div>
                            <div class="nav nav-tabs">
                                <div class="mt-element-step" >
                                    <div class="row step-thin">

                                        <div class="col-md-4 bg-grey mt-step-col active" id="nav_tab1">
                                            <div class="mt-step-number bg-white font-grey">1</div>
                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.basicInfo"></font></div>

                                        </div>
                                        <div class="col-md-4 bg-grey mt-step-col" id="nav_tab2">
                                            <div class="mt-step-number bg-white font-grey">2</div>
                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.group"></font></div>

                                        </div>
                                        <div class="col-md-4 bg-grey  mt-step-col " id="nav_tab3">
                                            <div class="mt-step-number bg-white font-grey">3</div>
                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.FINISH"></font></div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-content">
                                <div class="tab-pane active" id="tab1">
                                    <form action="#" class="horizontal-form" id="userModalInfoForm" >
                                        <div class="form-body">
                                            <div class="row space">
                                                <div class="col-md-6 col-sm-6">
                                                    <label for="registerId" class="col-md-3 control-label" ><font key="i18n.LOGIN"></font>ID:<span class="required">*</span></label>

                                                    <div class="col-md-7">
                                                        <input type="text" class="form-control"  id="registerId" placeholder="请输入ID" required="required">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                                <div class="col-md-6 col-sm-6">
                                                    <label for="registerName" class="col-md-3 control-label" ><font key="i18n.name"></font>:<span class="required">*</span></label>

                                                    <div class="col-md-7">
                                                        <input type="text" class="form-control" id="registerName" placeholder="请输入姓名" required="required">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                            </div>

                                            <!--/row-->
                                            <div class="row space">
                                                <div class="col-md-6 col-sm-6 needToHide">
                                                    <label for="registerPwd" class="col-md-3 control-label" ><font key="i18n.userWindowPassword"></font>:<span class="required">*</span></label>

                                                    <div class="col-md-7">
                                                        <input type="password" class="form-control" id="registerPwd" placeholder="请输入密码" required="required">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                                <div class="col-md-6 col-sm-6 needToHide">
                                                    <label for="registerCheckPwd" class="col-md-3 control-label" ><font key="i18n.userWindowConfigPas"></font>:<span class="required">*</span></label>

                                                    <div class="col-md-7">
                                                        <input type="password" class="form-control" id="registerCheckPwd" placeholder="请确认密码" required="required">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                            </div>

                                            <!--/row-->
                                            <div class="row space">
                                                <div class="col-md-6 col-sm-6">
                                                    <label for="registerEmail" class="col-md-3 control-label" ><font key="i18n.Email"></font>:<span class="required">*</span></label>

                                                    <div class="col-md-7">
                                                        <input type="text" class="form-control" id="registerEmail" placeholder="请输入邮箱" required="required">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                                <div class="col-md-6 col-sm-6">
                                                    <label for="registerTel" class="col-md-3 control-label" ><font key="i18n.tel"></font>:</label>

                                                    <div class="col-md-7">
                                                        <input type="text" class="form-control" id="registerTel" placeholder="请输入电话">
                                                    </div>
                                                </div>
                                                <!--/span-->
                                            </div>
                                            <!--/row-->
                                            <div class="row space">
                                                <div class="col-md-6 col-sm-6">
                                                    <label for="registerReporter" class="col-md-3 control-label" ><font key="i18n.reportTo"></font>:</label>

                                                    <div class="col-md-7">
                                                        <div class="input-group">
                                                            <input  class="form-control" type="text"  id="registerReporter" placeholder="请选择汇报人" />
                                                                    <span class="input-group-btn">
                                                                    <button  class="btn btn-success" type="button" id="userSelect_reporter"><font key="i18n.select"></font></button>
                                                                     </span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!--/span-->

                                            </div>
                                            <!--/row-->
                                            <div class="row space">
                                                <div class="col-md-6 col-sm-6">
                                                    <label  class="col-md-3 control-label" ><font key="i18n.status"></font>:</label>

                                                    <div class="col-md-5">
                                                        <div class="mt-radio-inline">
                                                            <label class="mt-radio">
                                                                <input type="radio" name="optionsRadios"  value="active" id="userStatus_1" checked> <font key="i18n.active"></font>
                                                                <span></span>
                                                            </label>
                                                            <label class="mt-radio">
                                                                <input type="radio" name="optionsRadios"  value="inactive"id="userStatus_2" > <font key="i18n.inactive"></font>
                                                                <span></span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <!--/span-->

                                                </div>
                                            </div>
                                        </div>
                                    </form>
                                    <p><font key="i18n.requireMsg"></font></p>
                                    <div class="col-md-offset-8">
                                        <button type="button" class="btn btn-success next-btn" id="userNext-btn"><font key="i18n.nextStep"></font></button>
                                    </div>
                                </div>

                                <div class="tab-pane" id="tab2" >
                                    <div class="row">
                                        <div class="col-lg-12 col-xs-12 col-sm-12">
                                            <div class="portlet light bordered">
                                                <div class="portlet-title">

                                                    <div class="actions">
                                                        <a href="#" class="btn btn-circle green btn-outline btn-sm" id="userAddGroup">
                                                            <i class="fa fa-user-plus"></i> <font key="i18n.add"></font> </a>
                                                        <a href="#" class="btn btn-circle green btn-outline btn-sm" id="userDeleteGroup">
                                                            <i class="fa fa-user-times"></i> <font key="i18n.delete"></font> </a>
                                                    </div>
                                                </div>
                                                <div class="portlet-body">
                                                    <div id = "user_createListAlert"></div>
                                                    <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="userGroupList">
                                                        <thead>
                                                        <tr class="bg-green text-white" >
                                                            <th class="col-md-1 col-sm-1 col-xs-1">
                                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                                                                    <input type="checkbox" class="group-checkable item_check" id="item_check" data-set="#example .checkboxes" />
                                                                    <span></span>
                                                                </label>
                                                            </th>

                                                            <th>ID</th>
                                                            <th> <font key="i18n.name"></font> </th>
                                                            <th> <font key="i18n.status"></font> </th>
                                                            <th> <font key="i18n.description"></font> </th>

                                                        </tr>
                                                        </thead>
                                                        <tbody id="userTbMain">

                                                        </tbody>

                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-success up-btn"><font key="i18n.preStep"></font></button>
                                        <button type="button" class="btn btn-primary " id="{{id}}"><font key="i18n.SAVE"></font></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}

