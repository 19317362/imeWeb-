{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}}">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font key="i18n.group"></font></span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                        <div class="portlet-body">
                            <div id = "group_listAlert"></div>
                            <div class="nav nav-tabs">
                                <div class="mt-element-step" >
                                    <div class="row step-thin">

                                        <div class="col-md-4 bg-grey mt-step-col active" id="nav_tab1">
                                            <div class="mt-step-number bg-white font-grey">1</div>
                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.basicInfo"></font></div>

                                        </div>
                                        <div class="col-md-4 bg-grey mt-step-col" id="nav_tab2">
                                            <div class="mt-step-number bg-white font-grey">2</div>
                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.member"></font></div>

                                        </div>
                                        <div class="col-md-4 bg-grey  mt-step-col " id="nav_tab3">
                                            <div class="mt-step-number bg-white font-grey">3</div>
                                            <div class="mt-step-title uppercase font-grey-cascade"><font key="i18n.FINISH"></font></div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-content">
                                <div class="tab-pane active" id="tab1">
                                    <form class="form-horizontal" role="form" id="myModalGroupForm">
                                        <div class="form-group">
                                            <label for="memberId" class="col-md-3 control-label" >ID:<span class="required">*</span></label>

                                            <div class="col-md-5">
                                                <input type="text" class="form-control" id="memberId" placeholder="请输入ID">
                                            </div>
                                        </div>
                                        <div class="form-group">

                                            <label for="name" class="col-md-3 control-label" ><font key="i18n.DocName"></font>:<span class="required">*</span></label>
                                            <div class="col-md-5">
                                                <input type="text" class="form-control" id="name" placeholder="请输入名称">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="desc" class="col-md-3 control-label" ><font key="i18n.description"></font>:</label>
                                            <div class="col-md-5">
                                                <textarea class="form-control" rows="3" id="desc" placeholder="请输入说明"></textarea>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <div class="col-md-offset-3 col-md-5">
                                                <button type="button" class="btn btn-success next-btn pull-right" id="groupNext-btn"><font key="i18n.nextStep">下一步</font></button>
                                            </div>
                                        </div>
                                    </form>
                                </div>

                                <div class="tab-pane" id="tab2" >
                                    <div class="row">
                                        <div class="col-lg-12 col-xs-12 col-sm-12">
                                            <div class="portlet light bordered">
                                                <div class="portlet-title">

                                                    <div class="actions">
                                                        <a href="#" class="btn btn-circle green btn-outline btn-sm" id="addMem">
                                                            <i class="fa fa-user-plus"></i> <font key="i18n.add"></font></a>
                                                        <a href="#" class="btn btn-circle green btn-outline btn-sm" id="deleteMem">
                                                            <i class="fa fa-user-times"></i> <font key="i18n.delete"></font> </a>
                                                    </div>
                                                </div>
                                                <div class="portlet-body">
                                                    <!--<div class="huai">2</div>-->
                                                    <!--<input type="text" value="mashr" id="ccc">-->
                                                    <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="Memberlist">
                                                        <thead>
                                                        <tr class="bg-green text-white" >
                                                            <th class="col-md-1 col-sm-1 col-xs-1">
                                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                                                                    <input type="checkbox" class="group-checkable item_check_one"  data-set="#example .checkboxes" />
                                                                    <span></span>
                                                                </label>
                                                            </th>

                                                            <th>ID</th>
                                                            <th> <font key="i18n.DocName"></font> </th>
                                                            <th> <font key="i18n.description"></font></th>

                                                        </tr>
                                                        </thead>
                                                        <tbody id="tbMain">

                                                        </tbody>

                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="modal-footer">
                                        <button type="button" class="btn btn-success up-btn"><font key="i18n.preStep"></font></button>
                                        <button type="button" class="btn btn-primary  saveGroupMem"><font key="i18n.SAVE"></font></button>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}

