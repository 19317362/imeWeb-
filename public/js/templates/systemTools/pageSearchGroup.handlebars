{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}}">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet  ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font key='i18n.searchPrincipal'></font> </span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                        <div class="row">
                            <div class="col-md-12 ">
                                <form class="form-horizontal" role="form">
                                    <div class="form-body">
                                        <div class="row">
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label"><font key="i18n.group"></font></label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" id="type" value="组" readonly>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">ID</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" id="principalId" placeholder="请输入ID">
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label"><font key="i18n.name"><font key="i18n.name"></font> </font></label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" id="name" placeholder="请输入name">
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </form>
                                <form class="form-horizontal" role="form">
                                    <div class="form-group col-md-4" style="height: 0">
                                    </div>
                                    <div class="form-group col-md-4" style="height: 0">
                                    </div>
                                    <div class="form-group col-md-4">
                                        <button type="button" id="searchGroup" class="btn  green mt-ladda-btn ladda-button pull-right" data-style="slide-right" style="">
                                            <span class="ladda-label"> <i class="fa  fa-search"></i> <font key="i18n.search">搜索</font></span>
                                        </button>
                                    </div>
                                </form>

                            </div>
                            <!--按钮效果-->

                        </div>


                        <!--表单组体-->
                        <div class="portlet-body refresh_area" style="min-height: 500px;">
                            <div id = "group_searchGroupListAlert"></div>
                            <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="Rembers">
                                <thead>
                                <tr class="bg-green text-white" >
                                    <th class="col-md-1 col-sm-1 col-xs-1">
                                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                                            <input type="checkbox" class="group-checkable item_check" id="item_check" data-set="#example .checkboxes" />
                                            <span></span>
                                        </label>
                                    </th>
                                    <th>ID</th>
                                    <th>  <font key="i18n.name"></font>  </th>
                                    <th> <font key="i18n.descriptioin"></font> </th>
                                    <th><font key="i18n.site"></font></th>
                                </tr>
                                </thead>

                            </table>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn default btn-outline" onclick="imeWeb.deleteChildPage();"><font key="i18n.wf_cancel"></font></button>
                            <button type="button" class="btn btn-success" id="{{pageName}}"><font key="i18n.add"></font></button>
                        </div>
                    </div>
                    <!-- END EXAMPLE TABLE PORTLET-->
                </div>
            </div>
        </div>
    </div>
{{/each}}