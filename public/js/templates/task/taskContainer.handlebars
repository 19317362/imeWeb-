{{#each this}}
    <div class="page-content page-content-base">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet ">
                    <ul class="page-task page-breadcrumb breadcrumb">
                        <!--<li>-->
                            <!--<a href="index.html"><font key="i18n.site"></font></a>-->
                            <!--<i class="fa fa-circle"></i>-->
                        <!--</li>-->
                        <!--<li>-->
                            <!--<span class="active"><font key="i18n.task"></font></span>-->
                        <!--</li>-->
                    </ul>

                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font key="i18n.task"></font> </span>
                            </div>
                            <!--按钮-->
                            <div class="actions">
                                <div class="btn-group btn-group-devided " data-toggle="buttons">
                                    <button class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-25" id="task_batchCompletion">
                                        <i class="fa fa-paper-plane-o"></i> 批量完成
                                    </button>
                                    <button type="button" id="refresh_task" class="btn  green mt-ladda-btn ladda-button btn-circle" data-style="slide-right">
                                        <span class="ladda-label"> <i class="fa fa-repeat"></i> <font key="i18n.refresh">刷新</font></span>
                                        <span class="ladda-spinner"></span><div class="ladda-progress" style="width: 0px;"></div></button>
                                </div>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-12 ">
                                <form class="form-horizontal" role="form">
                                    <div class="form-body">
                                        <div class="row">
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">文档名称</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" >
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">任务名称</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" id="principalId">
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">任务类型</label>
                                                <div class="col-md-9">
                                                    <div class="input-group">
                                                        <input id="docLog_softtypeInput" chosetype="" type="text" class="form-control" value="">
                                                        <div class="input-group-btn">
                                                            <button id="docLog_drapDown_softtype" type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown">
                                                                <i class="fa fa-angle-down"></i>
                                                            </button>
                                                            <ul class="dropdown-menu pull-right" id="" style="max-height: 200px;overflow-y: scroll;">
                                                                <li >
                                                                    <a href="javascript:;">CommonDoc</a>
                                                                </li>
                                                                <li >
                                                                    <a href="javascript:;">CommonDoc</a>
                                                                </li>
                                                                <li >
                                                                    <a href="javascript:;">CommonDoc</a>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">发起人</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" >
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">执行人</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" id="principalId" >
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">任务状态</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control" id="name" >
                                                </div>
                                            </div>

                                        </div>
                                        <div class="row">
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">启动时间</label>
                                                <div class="col-md-9">
                                                    <div class="input-group  date-picker input-daterange" >
                                                        <input id="task_startTimeCreate" type="text" class="form-control" name="">
                                                        <span class="input-group-addon"> - </span>
                                                        <input id="task_startTimeEnd" type="text" class="form-control" name="">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">截止时间</label>
                                                <div class="col-md-9">
                                                    <div class="input-group  date-picker input-daterange" data-orientation="right" data-date="10/11/2012">
                                                        <input id="task_endTimeCraate" type="text" class="form-control" name="">
                                                        <span class="input-group-addon"> - </span>
                                                        <input id="task_endTimeEnd" type="text" class="form-control" name="">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>
                                <form class="form-horizontal" role="form">
                                    <div class="form-group col-md-4" style="height: 0">
                                    </div>
                                    <div class="form-group col-md-4" style="height: 0">
                                    </div>
                                    <div class="form-group col-md-4">
                                        <button type="button" id="searchGroup" class="btn  green mt-ladda-btn ladda-button pull-right" data-style="slide-right" style="">
                                            <span class="ladda-label"> <i class="fa  fa-search"></i> <font key="i18n.search">搜索</font></span>
                                        </button>
                                    </div>
                                </form>
                            </div>
                        </div>


                        <!--表单组体-->
                        <div class="portlet-body refresh_area" style="min-height: 500px;">
                            <div id = "task_baseListAlert"></div>
                            <table class="table table-striped table-bordered table-hover dt-responsive nowrap" id="task_baseList">
                                <thead>
                                <tr class="bg-green text-white" >
                                    <th class="all" style="width:10px;padding-left:30px;">
                                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " >
                                            <input type="checkbox" class="item_check"   style="display:none;"/>
                                            <span></span>
                                        </label>
                                    </th>
                                    <th class="col-md-1 col-sm-1 col-xs-1 all">紧急度</th>
                                    <th class="all"> 文档类型 </th>
                                    <th class="all"> 任务名称 </th>
                                    <th class="all"> 文档对象 </th>
                                    <th class="all"> 工作流类型 </th>
                                    <th class="all"> 发起人 </th>
                                    <th class="all"> 执行人 </th>
                                    <th class="all"> 启动时间 </th>
                                    <th class="all"> 截止时间 </th>
                                    <th class="all"> 任务状态 </th>
                                    <th class="none"></th>
                                </tr>
                                </thead>
                                <tbody>

                                </tbody>
                            </table>
                        </div>

                    </div>
                    <!-- END EXAMPLE TABLE PORTLET-->
                </div>
            </div>
        </div>
    </div>

{{/each}}