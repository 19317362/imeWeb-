{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}}">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> 重新分配任务</span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                        <div class="row">
                            <div class="col-md-12 ">
                                <form class="form-horizontal" role="form">
                                    <div class="form-body">
                                        <div class="row">
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">用户名</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control"   id="task_username">
                                                </div>
                                            </div>
                                            <div class="form-group col-md-4">
                                                <label class="col-md-3 control-label">用户ID</label>
                                                <div class="col-md-9">
                                                    <input type="text" class="form-control "  id="task_id">
                                                </div>
                                            </div>

                                            <div class="form-group col-md-4" >
                                                <div class="col-md-3 action">
                                                    <button type="button" id="check_taskSearchBtn" class="btn  green mt-ladda-btn ladda-button " data-style="slide-right" >
                                                        <span class="ladda-label"> <i class="fa  fa-search"></i> <font key="i18n.search"></font></span>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </form>

                            </div>
                            <!--按钮效果-->


                        </div>


                        <!--表单组体-->
                        <div class="portlet-body refresh_area" style="min-height:250px;">
                            <div id = "user_searchUserListAlert"></div>
                            <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="task_searchList">
                                <thead>
                                <tr class="bg-green text-white" >
                                    <th class="col-md-1 col-sm-1 col-xs-1">
                                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline " style="margin-left:20px;">
                                            <input type="checkbox" class="group-checkable item_check" id="item_check" data-set="#example .checkboxes" />
                                            <span></span>
                                        </label>
                                    </th>
                                    <th>名称</th>
                                    <th> ID </th>
                                    <th> 电话 </th>
                                    <th>邮箱</th>
                                </tr>
                                </thead>

                            </table>
                        </div>
                        <form class="form-horizontal" role="form">
                            <div class="form-body">
                                <div class="row">
                                    <div class="form-group col-md-7">
                                        <label class="col-md-2 control-label">说明</label>
                                        <div class="col-md-9">
                                            <textarea class="form-control" rows="3" id="task_desc" placeholder="请输入说明"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <!--<form class="form-horizontal" role="form" >-->
                            <!--<div class="form-group col-md-4">-->
                                <!--<label class="col-md-3 control-label">说明</label>-->
                                <!--<div class="col-md-6">-->
                                    <!--<textarea class="form-control" rows="3" id="task_desc" placeholder="请输入说明"></textarea>-->
                                <!--</div>-->
                            <!--</div>-->
                            <!--<div class="form-group">-->
                                <!--<label for="desc" class="col-md-3 control-label"><font key="i18n.description">说明</font>:</label>-->
                                <!--<div class="col-md-5">-->
                                    <!--<textarea class="form-control" rows="3" id="task_desc" placeholder="请输入说明"></textarea>-->
                                <!--</div>-->
                            <!--</div>-->
                        <!--</form>-->
                        <br/>
                        <div class="modal-footer">
                            <button type="button" class="btn default btn-outline" onclick="imeWeb.deleteChildPage();"><font key="i18n.wf_cancel"></font></button>
                            <button type="button" class="btn btn-success" id="{{id}}">确定</button>
                        </div>
                    </div>
                    <!-- END EXAMPLE TABLE PORTLET-->
                </div>
            </div>
        </div>
    </div>
{{/each}}