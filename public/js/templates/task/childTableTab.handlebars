{{#each this}}
<div class="tabbable-line">
    <div class="actionTools">

    </div>
    <ul class="nav nav-tabs">
        <li></li>
        <li class="active">
            <a href="#taskTab_1" data-toggle="tab">其他信息</a>
        </li>
        <li >
            <a href="#taskTab_2" data-toggle="tab" id="taskTab_nameBtn">任务名称</a>
        </li>
        <li >
            <a href="#taskTab_3" data-toggle="tab" id="task_approvalHistoryBtn">审批历史</a>
        </li>
        <li >
            <a href="#taskTab_4" data-toggle="tab"  id="task_forwardingRecordBtn" >转发记录</a>
        </li>
        <li></li>
    </ul>
    <div class="tab-content" style="padding:0;">
        <div class="tab-pane active baseTab" id="taskTab_1">
            <div class="portlet light bordered" style="min-height:250px">
                <div class="portlet-body">
                    <div class="row">
                        {{#each data}}
                            <div class="col-lg-6 col-md-6">
                                <td>  {{{title}}} : </td>
                                <td>  {{{data}}}  </td>
                            </div>
                        {{/each}}
                    </div>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="taskTab_2">
            <div class="portlet light bordered" style="min-height:250px">
                <div class="portlet-body">
                    <div class="row">
                        <div class="col-md-10">
                            <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive " id="task_updateRolesHead">
                                <thead>
                                <tr class="bg-green text-white">
                                    <th>角色/用户</th>
                                    <th> 操作 </th>
                                </tr>
                                </thead>
                                <tbody id="task_rolesTree">


                                </tbody>
                            </table>

                        </div>
                        <form class="form-horizontal" role="form">
                            <div class="form-body">

                                <div class="row">
                                    <div class="form-group col-md-7">
                                        <label class="col-md-2 control-label">说明:<span class="required">*</span></label>
                                        <div class="col-md-9">
                                            <textarea class="form-control" rows="3" id="task_desc" placeholder="请输入说明"></textarea>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-md-7">
                                        <label class="col-md-2 control-label">任务分支:<span class="required">*</span></label>
                                        <div class="col-md-9">
                                            <label class="mt-checkbox mt-checkbox-outline margin-top-10">
                                                <input id="task_containerAllUser" type="checkbox" class="input-small">完成
                                                <span></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="form-group col-md-7">
                                        <label class="col-md-2 control-label"></label>
                                        <div class="col-md-9">
                                            <button type="button" class="btn btn-success next-btn pull-right" id="groupNext-btn"><font key="">确定</font></button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="taskTab_3">
            <div class="portlet light bordered" style="min-height:250px">
                <div class="portlet-body">
                    <div class="row">
                        <ul class="nav nav-tabs">
                            <li></li>
                            <li class="active">
                                <a href="#taskChildTab_1" data-toggle="tab">工作流</a>
                            </li>
                            <li >
                                <a href="#taskChildTab_2" data-toggle="tab" id="task_flowBtn">流程图</a>
                            </li>

                            <li></li>
                        </ul>
                        <div class="tab-content" style="padding:0;">
                            <div class="tab-pane active baseTab" id="taskChildTab_1">
                                <div class="portlet light bordered col-md-10" style="min-height:250px;">
                                    <div class="portlet-body">
                                        <div class="row">
                                            <!--主体标题开始-->
                                            <div class="portlet-title">
                                                <div class="actions">
                                                    <!--<a href="javascript:;" class="btn btn-sm btn-circle green easy-pie-chart-reload" id="refresh_systemInfoList">-->
                                                    <!--<i class="fa fa-repeat"></i> 刷新 </a>-->
                                                    <button type="button"  id="refresh_taskAssignmentsList" class="btn  green mt-ladda-btn ladda-button btn-circle" data-style="slide-right" >
                                                        <span class="ladda-label"> <i class="fa fa-repeat"></i> <font key="i18n.refresh">刷新</font></span>
                                                    </button>
                                                </div>
                                            </div>


                                            <div class="portlet-body refresh_childarea">
                                                <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="taskAssignmentsList">
                                                    <thead>
                                                    <tr class="bg-green text-white" >
                                                        <th class="col-md-3">名称</th>
                                                        <th> 工作负责人 </th>
                                                        <th> 状态 </th>
                                                        <th> 执行人 </th>
                                                        <th> 任务分支 </th>
                                                        <th> 说明 </th>
                                                        <th> 创建时间 </th>
                                                    </tr>
                                                    </thead>
                                                    <tbody id="taskAssignmentsList_tbody"></tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-pane col-md-10" id="taskChildTab_2">
                                <div class="actions margin-bottom-10">
                                    <div class="btn-group btn-group-devided " data-toggle="buttons">
                                        <button class="btn btn-transparent green btn-outline btn-circle btn-sm margin-right-10" id="task_zoomInBtn">
                                            <input type="radio" name="options" class="toggle"  > 放大
                                        </button>
                                        <label class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-10 " id="task_zoomOutBtn">
                                            <input type="radio" name="options" class="toggle" >缩小</label>
                                        <label class="btn btn-transparent green btn-outline btn-circle btn-sm  margin-right-10 " id="task_fitBtn">
                                            <input type="radio" name="options" class="toggle" id="option3">适中</label>
                                    </div>
                                </div>
                                <div class="portlet light bordered" style="min-height:250px;overflow-y:auto;max-height:300px;">
                                    <div class="portlet-body">
                                        <div class="row" id="task_4Container">

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                </div>
            </div>
        </div>
        <div class="tab-pane" id="taskTab_4">
            <div class="portlet light bordered" style="min-height:250px">
                <div class="portlet-body">
                    <div class="row" >

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

{{/each}}


