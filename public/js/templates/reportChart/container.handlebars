<!--工作流容器-->
{{#each this}}
<div class="page-content  page-content-base" id='reportchart'>
    <ul class="page-breadcrumb breadcrumb">
        <li>
            <a href="index.html">首页</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span class="active"><font>报表</font></span>
        </li>
    </ul>
    <div class='row'>
        <!--表格插入区域-->
        <div class="col-md-12">
            <div class="portlet light bordered">
             <div class="portlet-title tabbable-line">
                <div class="caption">
                 <button type="button" class="btn  mt-ladda-btn ladda-button myblockbutton  blockui_TaskInit" data-style="contract" data-spinner-color="#697882">
                    <span class="ladda-label"> <i class="icon-refresh"></i></span>
                    <span class="ladda-spinner"></span>
                    <span class="ladda-spinner"></span>
                    <span class="ladda-spinner"></span>
                </button>
                <span class="caption-subject font-dark bold uppercase">报表</span>
            </div>
           <!--  <ul class="nav nav-tabs">
                <li class="active">
                    <a href="#workflow_instance" data-toggle="tab"> 流程实例</a>
                </li>
                <li>
                    <a href="#workflow_identify" data-toggle="tab">流程定义</a>
                </li>
            </ul> -->
        </div>
        <div class="portlet-body" style="min-height:300px">
            <form class="form-horizontal" role="form">
                <div class="form-body myform-body">
                    <div class='row'>
                        <div class="form-group col-md-6">
                            <label class="col-md-2 control-label">报表类型</label>
                            <div class="col-md-10">
                                <select id="chartType" class="form-control">
                                    <option value='' disabled selected>--请选择--</option>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                    <option>Option 4</option>
                                    <option>Option 5</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="col-md-2 control-label">数据源</label>
                            <div class="col-md-10">
                                <select id="chartDs" class="form-control">
                                    <option value='' >--请选择--</option>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                    <option>Option 4</option>
                                    <option>Option 5</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="col-md-2 control-label">轴标签</label>
                            <div class="col-md-10">
                                <select  id="axisLabel" class="form-control">
                                    <option value='' disabled selected>--请选择--</option>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                    <option>Option 4</option>
                                    <option>Option 5</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="col-md-2 control-label">系列名称</label>
                            <div class="col-md-10">
                                <select id="seriesName" class="form-control">
                                    <option  value=''>--请选择--</option>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                    <option>Option 4</option>
                                    <option>Option 5</option>
                                </select>
                            </div>
                        </div>
						<div class="form-group col-md-6">
                            <label class="col-md-2 control-label">系列值</label>
                            <div class="col-md-10">
                                <select id="seriesValue" multiple="multiple" class="form-control multiple">
                                    <option   value='' disabled selected>--请选择--</option>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                    <option>Option 4</option>
                                    <option>Option 5</option>
                                </select>
                            </div>
                        </div>
                         <div class="form-group col-md-6">
                            <label class="col-md-2 control-label"></label>
                            <div class="col-md-10">
                            </div>
                        </div>
                        <div class="form-group col-md-6">
                            <label class="col-md-2 control-label"></label>
                            <div class="col-md-10">
                               <button type="button" class="btn green search_object  pull-right" id='reportchart_container_next'>下一步</button>
                            </div>
                        </div>
                 </div>
             </div>
         </form>
     </div>
 </div>
</div>
</div>
</div>
