<!--工作流容器-->
{{#each this}}
<div class="page-content parent-{{parentName}} {{pageName}}" id='reportchartnext'>
    <ul class="page-breadcrumb breadcrumb">
        <li>
            <a href="index.html">首页</a>
            <i class="fa fa-circle"></i>
        </li>
        <li>
            <span class="active"><font>报表</font></span>
        </li>
    </ul>
    <div class='row'>
        <!--表格插入区域-->
        <div class="col-md-12">
            <div class="portlet light bordered">
             <div class="portlet-title tabbable-line">
                <div class="caption">
                 <button type="button" class="btn  mt-ladda-btn ladda-button myblockbutton  blockui_TaskInit" data-style="contract" data-spinner-color="#697882">
                    <span class="ladda-label"> <i class="icon-refresh"></i></span>
                    <span class="ladda-spinner"></span>
                    <span class="ladda-spinner"></span>
                    <span class="ladda-spinner"></span>
                </button>
                <span class="caption-subject font-dark bold uppercase">数据范围</span>
            </div>
             <div class="actions">
              {{> common/protletTitleTools protletTitleTools}}
          </div>

        </div>
        <div class="portlet-body" style="min-height:300px">
            <form class="form-horizontal" role="form">
                <div class="form-body myform-body">
                    <div class='row'>
                        <div class="form-group col-md-4">
                            <label class="col-md-4 control-label">数据类别</label>
                            <div class="col-md-8">
                                <select id="columnType" class="form-control">
                                    <option value='' disabled selected>--请选择--</option>
                                    <option>Option 1</option>
                                    <option>Option 2</option>
                                    <option>Option 3</option>
                                    <option>Option 4</option>
                                    <option>Option 5</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group col-md-3">
                            <label class="col-md-4 control-label">操作符</label>
                            <div class="col-md-8">
                                <select id="columnOperate" class="form-control">
                                    <option value='' >--请选择--</option>
                                    <option value=">">大于</option>
                                    <option value="=">等于</option>
                                    <option value="<">小于</option>
									<option value="IN">选择</option>
									<option value="like">包含</option>
                                    <option value="<=">小于等于</option>
									<option value=">=">大于等于</option>
									<option value="!=">不等于</option>
                                </select>
                            </div>
                        </div>
						<div class="form-group col-md-4">
                            <label class="col-md-3 control-label">限制值</label>
                            <div class="col-md-9">
                               <input id="columnValue" type="text" class="form-control"/>
                            </div>
                        </div>
						<div class="form-group col-md-1">
							<button type="button"  class="btn green" id="addLimitCol"><font key="i18n.add"></font></button>
						</div>
                 </div>
             </div>
         </form>
		 <!-- BEGIN EXAMPLE TABLE PORTLET-->
			<div class="portlet ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font key=''>条件列表</font> </span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>
                        <!--表单组体-->
                        <div class="portlet-body">
                            <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive" id="limit_list">
                                <thead>
                                <tr class="bg-green text-white" >
                                    <th><font key="">数据类别</font></th>
                                    <th><font key="">操作符</font></th>
                                    <th><font key="">限制值</font> </th>   
                                </tr>
                                </thead>
                            </table>
                        </div>
             </div>
		 <!-- END EXAMPLE TABLE PORTLET-->
			 <div class="form-group col-md-6">
				<label class="col-md-2 control-label"></label>
				<div class="col-md-10">
				</div>
			 </div>
			<div class="form-group col-md-6">
				<label class="col-md-2 control-label"></label>
				<div class="col-md-10">
				   <button type="button" class="btn green search_object  pull-right" id="confirmLimitCol">提交</button>
				</div>
			</div>
		</div>
     </div>
 </div>
</div>
</div>
</div>
{{/each}}