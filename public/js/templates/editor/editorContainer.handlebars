                            </div>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="tabbable tabbable-tabdrop height-all">
                            <div class="tab-content height-all">
                                {{> editor/tabContent editorContentData}}
                                {{!-- <div class="tab-pane active" id="tab1">
                                    <iframe frameborder='0' width="100%" height="850" src="http://localhost/imeWeb/editor.html?folderId=1361&containerId=com.imecms.container.Container:131&type=plugin&oid=com.imecms.doc.XMLDocument:39055&timeId=1491792980548"></iframe>
                                </div> --}}
