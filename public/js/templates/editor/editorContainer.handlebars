{{#each this}}
    <div class="page-content {{pageName}}">
        <div class="row height-all">
            <div class="col-md-12 height-all">
                <div class="portlet light bordered height-all">
                    <div class="portlet-title">
                        <div class="row">
                            <div class="doc-path col-md-8">
                                <ul class="page-breadcrumb breadcrumb">
                                    <li>
                                        <a href="javascript:imeWeb.changeRoute('/imeWeb/home');"><font key="">首页</font></a>
                                        <i class="fa fa-circle"></i>
                                    </li>
                                    <li>
                                        <span class="active"><font key="">编辑器</font></span>
                                    </li>
                                </ul>
                            </div>
                            <div class="editor-window-tools col-md-4">
                                <a class="fa fa-window-maximize editor-maximize pull-right"></a>
                                <a class="fa fa-window-restore editor-restore pull-right" style="display:none;"></a>
                                <a class="fa fa-window-minimize editor-minimize pull-right"></a>
                            </div>
                        </div>
                        <div class="row">
                            <div class="doc-edit-tab col-md-12">
                                <ul class="nav nav-pills">
                                     {{> editor/tabNav editorNavData}}
