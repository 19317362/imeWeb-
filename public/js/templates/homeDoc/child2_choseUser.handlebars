{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}} page-content-base2">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet ">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption font-green bordered">
                                <i class="icon-settings font-green"></i>
                                <span class="caption-subject bold uppercase"> <font
                                        key="i18n.searchPrincipal"></font></span>
                            </div>
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                        <div class="portlet-body">
                            <div class="row">
                                <div class="col-md-12 ">
                                    <form class="form-horizontal" role="form">

                                        <!--____________________________________-->
                                        <div class="form-group col-md-4">
                                            <label class="col-md-3 control-label"><font key="i18n.softtype"></font></label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" value='用户' disabled>
                                            </div>
                                        </div>

                                        <div class="form-group col-md-4">
                                            <label  for="homeDoc_principalId" class="col-md-3 control-label"><font key="i18n.id"></font></label>
                                            <div class="col-md-9">
                                                <input  id="homeDoc_principalId" type="text" class="form-control">
                                            </div>
                                        </div>

                                        <div class="form-group col-md-4">
                                            <label  for="homeDoc_searchName" class="col-md-3 control-label"><font
                                                    key="i18n.name"></font>:</label>
                                            <div class="col-md-9">
                                                <input  id="homeDoc_searchName" type="text" class="form-control">
                                            </div>
                                        </div>

                                        <div class="form-group col-md-12">
                                                <button id="homeDoc_searchUser" type="button" class="btn green pull-right"><font key="i18n.search"></font></button>
                                        </div>

                                    </form>


                                </div>
                                <!--按钮效果-->


                            </div>

                            <!--表格-->
                            <div class="portlet-body">

                                <table class="table table-striped table-bordered table-condensed table-hover table-checkable order-column table-responsive"
                                       id="usersDataShow">
                                    <thead>
                                    <tr class="bg-green text-white">
                                        <th class="col-md-1 col-sm-1 col-xs-1">

                                        </th>
                                        <th>ID</th>
                                        <th><font key="i18n.name"></font></th>
                                        <th><font key="i18n.descriptioin"></font></th>
                                        <th><font key="i18n.softtype"></font></th>
                                    </tr>
                                    </thead>
                                    <tbody id="modal_searchUsersBox">
                                    <!--插入数据-->
                                    </tbody>

                                </table>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-success" id="modal_searchUser_conform"
                                        modal_searchType="{{id}}"><font key="i18n.conform"></font>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}
