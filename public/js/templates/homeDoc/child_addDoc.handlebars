{{#each this}}
    <div class="page-content parent-{{parentName}} {{pageName}} page-content-base2">
        <div class='row'>
            <div class="col-md-12">
                <div class="portlet light bordered">
                    <div class="portlet-title tabbable-line">
                        <div class="caption">
                            <button type="button" class="btn  mt-ladda-btn ladda-button myblockbutton  blockui_TaskInit"
                                    data-style="contract" data-spinner-color="#697882">
                                <span class="ladda-label"> <i class="icon-refresh"></i></span>
                                <span class="ladda-spinner"></span>
                                <span class="ladda-spinner"></span>
                                <span class="ladda-spinner"></span>
                            </button>
                            <span class="caption-subject font-dark bold uppercase">对象查询</span>
                        </div>

                        <div class="actions">
                            {{> common/protletTitleTools protletTitleTools}}
                        </div>

                    </div>
                    <div class="portlet-body">
                        <form class="form-horizontal" role="form">
                            <div class="form-body myform-body">
                                <div class='row'>

                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">关键字</label>
                                        <div class="col-md-9">
                                            <input id="homeDoc_keyword" type="text" class="form-control">
                                        </div>
                                    </div>

                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">内容</label>
                                        <div class="col-md-9">
                                            <input id="homeDoc_content" type="text" class="form-control">
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="control-label col-md-3">创建时间起止</label>
                                        <div class="col-md-9">
                                            <div class="input-group  date-picker input-daterange"
                                                 data-orientation="right" data-date="10/11/2012">
                                                <input id="homeDoc_begincreated" type="text" class="form-control"
                                                       name="from">
                                                <span class="input-group-addon"> 至</span>
                                                <input id="homeDoc_endcreated" type="text" class="form-control"
                                                       name="to">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">创建人</label>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input id="homeDoc_creator" class="form-control" type="text">
                                                <span class="input-group-btn">
                                    <button searchType="creator" class="btn btn-success modalBtn_searchUser"
                                            type="button"> 选择
                                    </button>
                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">存储库</label>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input id="homeDoc_containerInp" type="text" class="form-control" id="input" value="">
                                                <div class="input-group-btn">
                                                    <button id="homeDoc_drapDown_container" type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" >
                                                        <i class="fa fa-angle-down"></i>
                                                    </button>
                                                    <ul  id="homeDoc_container" class="dropdown-menu pull-right" style="max-height: 200px;overflow-y: scroll;">
                                                        <!--插入-->
                                                    </ul>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">类型</label>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input id="homeDoc_softtypeInp" type="text" class="form-control" id="input" value="">
                                                <div class="input-group-btn">
                                                    <button id="homeDoc_drapDown_softtype" type="button" class="btn btn-success dropdown-toggle" data-toggle="dropdown" >
                                                        <i class="fa fa-angle-down"></i>
                                                    </button>
                                                    <ul id="homeDoc_softtype" class="dropdown-menu pull-right" style="max-height: 200px;overflow-y: scroll;">
                                                        <!--插入-->
                                                    </ul>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <label class="col-md-3 control-label">检出人</label>
                                        <div class="col-md-9">
                                            <div class="input-group">
                                                <input id="homeDoc_checkoutUser" class="form-control" type="text"
                                                       name="password">
                                                <span class="input-group-btn">
                                    <button searchType="checkoutUser" class="btn btn-success modalBtn_searchUser"
                                            type="button"> 选择
                                    </button>
                                </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group col-md-4">
                                        <div class="col-md-9 col-md-push-3">
                                            <div class="input-group">
                                                    <button type="button" id="homeDoc_search"class="btn  green mt-ladda-btn ladda-button" data-style="slide-right" >
                                                        <span class="ladda-label"> <i class="fa  fa-search"></i> <font key="i18n.search"></font></span>
                                                    </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <div id="homeDoc_searchRes_loadding" style="min-height: 500px;">
                            <table id="modal1_dataShow"
                                   class="table table-striped table-bordered table-hover table-checkable order-column"
                                   id="search_object">
                                <thead>
                                <tr class="bg-green">
                                    <th>
                                        <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                            <input id="modal_allChecked" type="checkbox"
                                                   class="group-checkable homeDoc_checkBox"/>
                                            <span></span>
                                        </label>
                                    </th>
                                    <th><font key="i18n.number"></font></th>
                                    <th><font key="i18n.DocName"></font></th>
                                    <th><font key="i18n.lifeCycle"></font></th>
                                    <th><font key="i18n.created"></font></th>
                                    <th><font key="i18n.creator"></font></th>
                                    <th><font key="i18n.softtype"></font></th>
                                </tr>
                                </thead>
                                <tbody id="homeDoc_searchRes">
                                <!--搜索结果数据-->
                                </tbody>

                            </table>

                            <div class="modal-footer" style="border-top: none;">
                                <button id="homeDoc_confChecked" type="button" class="btn green"><font
                                        key="i18n.conform"></font>
                                </button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}



