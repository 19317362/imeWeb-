{{! home页面容器 }}
{{#each this}}
    <div class="page-content">
        <div class="page-head">
            <div class="page-title">
                <h1> <font key="i18n.appName"></font>
                    <small></small>
                </h1>
            </div>
        </div>

		{{! 面包屑导航 }}
        <ul class="page-breadcrumb breadcrumb">
            <li>
                {{! 插入标签}}
            </li>
        </ul>

        <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="portlet light bordered">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="icon-cursor font-dark hide"></i>
                            <span class="caption-subject font-dark bold uppercase"><font key="i18n.myTask"></font></span>
                        </div>
                        <div class="actions">
                            <a href="javascript:;" class="btn btn-sm btn-circle red easy-pie-chart-reload">
                                <i class="fa fa-repeat"></i> <font key="i18n.refresh"></font> </a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="row">
                            <div class="col-md-12">
                                <table class="table table-striped table-bordered table-hover table-checkable order-column" id="myTask">
                                    <thead>
                                        <tr>
                                            <th>
                                                <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">
                                                <input type="checkbox" class="group-checkable" data-set="#sample_1 .checkboxes" />
                                                <span></span>
                                                </label>
                                            </th>
                                            <th> <font key="i18n.urgency"></font> </th>
                                            <th> <font key="i18n.taskName"></font> </th>
                                            <th> <font key="i18n.fileObject"></font> </th>
                                            <th> <font key="i18n.modified"></font> </th>
                                            <th> <font key="i18n.softtype"></font> </th>
                                            <th> <font key="i18n.assignDate"></font> </th>
                                            <th> <font key="i18n.deadline"></font> </th>
                                            <th> <font key="i18n.status"></font> </th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/each}}